    <style>
        @@import url('https://fonts.googleapis.com/css?family=Bangers&display=swap');

        :root {
            --effect: hover 1s linear infinite;
        }

        @@keyframes rotation {
            to {
                transform: rotate(360deg) translateX(-50%) translateY(-50%);
            }
        }

        * {
            cursor: url(https://www.howkteam.vn/Content/images/cursor/rocket_24.png), auto !important;
        }

        body {
            background-image: url('../../Image/background_game.jpg');
            background-repeat: no-repeat;
            background-size: cover;
        }

        .containerGame {
            height: 100%;
            width: 100%;
        }

        @@keyframes move-background {
            from {
                -webkit-transform: translate3d(0px, 0px, 0px);
            }

            to {
                -webkit-transform: translate3d(1000px, 0px, 0px);
            }
        }

        @@-webkit-keyframes move-background {
            from {
                -webkit-transform: translate3d(0px, 0px, 0px);
            }

            to {
                -webkit-transform: translate3d(1000px, 0px, 0px);
            }
        }

        @@-moz-keyframes move-background {
            from {
                -webkit-transform: translate3d(0px, 0px, 0px);
            }

            to {
                -webkit-transform: translate3d(1000px, 0px, 0px);
            }
        }

        @@-webkit-keyframes move-background {
            from {
                -webkit-transform: translate3d(0px, 0px, 0px);
            }

            to {
                -webkit-transform: translate3d(1000px, 0px, 0px);
            }
        }

        .load {
            position: fixed;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
        }

            .load .contentLoad {
                background-color: black;
                margin-top: 150px;
                margin-left: 350px;
            }

            .load .stars {
                background: black url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/1231630/stars.png) repeat;
                position: absolute;
                top: 0;
                bottom: 0;
                left: 0;
                right: 0;
                display: block;
                z-index: 0;
            }

            .load .twinkling {
                width: 10000px;
                height: 100%;
                background: transparent url("https://s3-us-west-2.amazonaws.com/s.cdpn.io/1231630/twinkling.png") repeat;
                background-size: 1000px 1000px;
                position: absolute;
                right: 0;
                top: 0;
                bottom: 0;
                z-index: 2;
                -moz-animation: move-background 70s linear infinite;
                -o-animation: move-background 70s linear infinite;
                -webkit-animation: move-background 70s linear infinite;
                animation: move-background 70s linear infinite;
            }

            .load .clouds {
                width: 10000px;
                height: 100%;
                background: transparent url("https://s3-us-west-2.amazonaws.com/s.cdpn.io/1231630/clouds_repeat.png") repeat;
                background-size: 1000px 1000px;
                position: absolute;
                right: 0;
                top: 0;
                bottom: 0;
                z-index: 3;
                -moz-animation: move-background 150s linear infinite;
                -o-animation: move-background 150s linear infinite;
                -webkit-animation: move-background 150s linear infinite;
                animation: move-background 150s linear infinite;
            }

            .load img {
                height: 70vh;
                width: 70vh;
                position: absolute;
                z-index: 3;
                right: 20px;
            }

            .load .contentLoad h2 {
                font-family: 'Bangers', cursive;
                color: hsl(50, 50%, 50%);
                position: relative;
                z-index: 2;
                font-size: 2em;
                letter-spacing: 5px;
                text-transform: uppercase;
                transform: rotate(-10deg);
                text-shadow: 1px 1px hsl(50, 50%, 45%), 2px 2px hsl(50, 50%, 40%), 3px 3px hsl(50, 50%, 35%), 4px 4px hsl(50, 50%, 34%), 5px 5px hsl(50, 50%, 33%), 6px 6px hsl(50, 50%, 32%), 7px 7px hsl(50, 50%, 31%), 8px 8px hsl(50, 50%, 30%), 9px 9px hsl(50, 50%, 29%), 10px 10px hsl(50, 50%, 28%), 10px 10px 30px rgba(0,0,0,.7),
            }

            .load .contentLoad p {
                display: inline-block;
                text-transform: uppercase;
                text-align: center;
                font-size: 5em;
                font-family: arial;
                font-weight: bold;
                transform: scale(.5);
                color: #121212;
                -webkit-text-stroke: 2px gray;
            }

                .load .contentLoad p:nth-child(1) {
                    animation: var(--effect);
                }

                .load .contentLoad p:nth-child(2) {
                    animation: var(--effect) .125s;
                }

                .load .contentLoad p:nth-child(3) {
                    animation: var(--effect) .25s;
                }

                .load .contentLoad p:nth-child(4) {
                    animation: var(--effect) .375s;
                }

                .load .contentLoad p:nth-child(5) {
                    animation: var(--effect) .5s;
                }

                .load .contentLoad p:nth-child(6) {
                    animation: var(--effect) .675s;
                }

                .load .contentLoad p:nth-child(7) {
                    animation: var(--effect) .75s;
                }

        @@keyframes hover {
            0% {
                transform: scale(.5);
                color: #121212;
                -webkit-text-stroke: 2px gray;
            }

            20% {
                transform: scale(1);
                color: pink;
                -webkit-text-stroke: 3px red;
                filter: drop-shadow(0 0 1px black)drop-shadow(0 0 1px black)drop-shadow(0 0 3px red)drop-shadow(0 0 5px red)hue-rotate(10turn);
            }

            50% {
                transform: scale(.5);
                color: #121212;
                -webkit-text-stroke: 2px gray;
            }
        }

