            .planet_small .background_small {
                animation: translateBackground 40s infinite linear;
                background: url(https://1.bp.blogspot.com/-UUXaK5GCj-k/UcsKJRMgkVI/AAAAAAAACfM/sePP_H08JTQ/s1600/1.jpg) repeat-x;
                width: 300px;
                height: 300px;
                position: absolute;
